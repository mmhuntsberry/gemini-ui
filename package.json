{
  "name": "nielsen-media",
  "version": "0.0.0",
  "license": "MIT",
  "repository": "https://gitlab.com/nielsen-media/ma/design-libraries/gds-packages",
  "publishConfig": {
    "@nielsen-media:registry": "https://gitlab.com/api/v4/projects/40331883/packages/npm/"
  },
  "scripts": {
    "bootstrap": "npm-run-all \"build:*\"",
    "pre:deploy": "sed -i -e 's|/assets|assets|g' storybook/iframe.html",
    "deploy": "npm-run-all \"publish:*\"",
    "build:gds-utilities": "npx nx run gds-utilities:build",
    "build:gds-web": "npx nx run gds-web:build && npm run copy:gds-web-dts",
    "build:gds-react": "npx nx run gds-react:build && npm run copy:gds-react-dts",
    "build:gds-types": "npx tsc --build packages/gds-types/tsconfig.json",
    "clean": "rm -rf node_modules && rm package-lock.json",
    "clean:cache": "npx nx clear-cache",
    "clean:dist": "find packages -type d -name 'dist' -exec rm -rf {} \\;",
    "copy:gds-web-dts": "copyfiles -u 1 \"packages/gds-web/src/index.d.ts\" packages/gds-web/dist --flat",
    "copy:gds-react-dts": "copyfiles -u 1 \"packages/gds-react/src/index.d.ts\" packages/gds-react/dist --flat",
    "dev:gds-web": "npx nx run gds-web:storybook",
    "dev:gds-react": "npx nx run gds-react:storybook",
    "prod:gds-react": "npx nx run gds-react:build-storybook",
    "prod:gds-web": "npx nx run gds-web:build-storybook",
    "publish:gds-react": "cd packages/gds-react; npm publish ./dist; cd ../..",
    "publish:gds-core": "cd packages/gds-core; npm publish; cd ../..",
    "publish:gds-types": "cd packages/gds-types; npm publish ./dist; cd ../..",
    "publish:gds-utilities": "cd packages/gds-utilities; npm publish ./dist; cd ../..",
    "publish:gds-web": "cd packages/gds-web; npm publish ./dist; cd ../.."
  },
  "private": true,
  "devDependencies": {
    "@babel/core": "7.12.13",
    "@babel/preset-typescript": "7.12.13",
    "@fullhuman/postcss-purgecss": "^4.1.3",
    "@nrwl/cli": "14.5.10",
    "@nrwl/cypress": "14.7.12",
    "@nrwl/devkit": "^14.6.2",
    "@nrwl/eslint-plugin-nx": "14.5.10",
    "@nrwl/jest": "14.5.10",
    "@nrwl/linter": "14.5.10",
    "@nrwl/node": "^14.6.2",
    "@nrwl/react": "^14.5.10",
    "@nrwl/storybook": "^14.7.11",
    "@nrwl/tao": "^14.6.2",
    "@nrwl/web": "14.5.10",
    "@nrwl/workspace": "^14.5.10",
    "@storybook/addon-essentials": "~6.5.12",
    "@storybook/builder-vite": "^0.2.4",
    "@storybook/builder-webpack5": "~6.5.12",
    "@storybook/core-server": "~6.5.12",
    "@storybook/manager-webpack5": "~6.5.12",
    "@storybook/react": "~6.5.12",
    "@storybook/web-components": "^6.5.12",
    "@svgr/webpack": "^5.4.0",
    "@testing-library/react": "13.3.0",
    "@types/d3": "^7.4.0",
    "@types/jest": "27.4.1",
    "@types/node": "16.11.7",
    "@types/react": "18.0.17",
    "@types/react-dom": "18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/parser": "^5.29.0",
    "babel-jest": "27.5.1",
    "babel-loader": "8.1.0",
    "copyfiles": "^2.4.1",
    "cypress": "^10.7.0",
    "eslint": "~8.15.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-cypress": "^2.10.3",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsx-a11y": "6.6.1",
    "eslint-plugin-react": "7.30.1",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-storybook": "^0.6.4",
    "jest": "27.5.1",
    "jest-environment-jsdom": "28.1.1",
    "nx": "14.5.10",
    "nx-plugin-vite": "^2.2.1",
    "postcss": "^8.4.16",
    "prettier": "^2.6.2",
    "raw-loader": "^4.0.2",
    "react-test-renderer": "18.2.0",
    "rollup-plugin-typescript2": "^0.33.0",
    "sass": "^1.54.7",
    "sass-loader": "^13.0.2",
